    "@heroicons/react": "^2.0.18",
    "@remix-run/cloudflare": "^1.16.1",
    "@remix-run/cloudflare-pages": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@stellar/design-system": "^1.0.0-beta.11",
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",
    "ahooks": "^3.7.7",
    "buffer-polyfill": "npm:buffer@^6.0.3",
    "cfw-easy-utils": "^1.0.3",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dayzed": "^3.2.3",
    "isbot": "^3.6.5",
