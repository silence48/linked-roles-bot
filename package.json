{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "db:seeds": "npx wrangler d1 execute DB --file ./db/schema.sql",
    "db:seeds:local": "npx wrangler d1 execute DB --file ./db/schema.sql --local",
    "wrangler": "wrangler pages dev ./public --ip 0.0.0.0 --local --persist --local-protocol https"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.0",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@remix-run/cloudflare": "^1.16.1",
    "@remix-run/cloudflare-pages": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",
    "buffer-polyfill": "npm:buffer@^6.0.3",
    "cross-env": "^7.0.3",
    "framer-motion": "^6",
    "joi": "^17.8.3",
    "model-one": "^0.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "stellar-base": "9.0.0-soroban.3"
  },
  "devDependencies": {
    "@albedo-link/intent": "latest",
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@stellar/freighter-api": "latest",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@walletconnect/sign-client": "latest",
    "better-sqlite3": "latest",
    "communi-design-system": "communiDAO/communi-design-system#update",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "react-qrcode-logo": "latest",
    "typescript": "^4.9.5",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=17.9.1"
  },
  "volta": {
    "node": "17.9.1"
  }
}
